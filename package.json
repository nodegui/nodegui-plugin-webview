{
  "name": "nodegui-plugin",
  "version": "1.0.0",
  "description": "A sample native plugin example for NodeGUI",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:addon",
    "build:addon": "cross-env CMAKE_BUILD_PARALLEL_LEVEL=8 cmake-js compile",
    "dev": "npm run build && qode dist/demo.js"
  },
  "author": "Atul R",
  "license": "MIT",
  "peerDependencies": {
    "@nodegui/nodegui": "*"
  },
  "dependencies": {
    "cmake-js": "^5.3.2",
    "cross-env": "^6.0.3",
    "node-addon-api": "^1.7.1"
  },
  "devDependencies": {
    "@nodegui/nodegui": "^0.6.5",
    "@nodegui/qode": "^1.0.6",
    "@types/node": "^12.12.8",
    "typescript": "^3.7.2"
  }
}
